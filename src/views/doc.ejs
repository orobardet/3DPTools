<a name="page-top" id="page-top" class="anchor"></a>
<a href="#page-top" class="page-top" style="display: none;"><%- __('Page top'); %></a>
<div class="docpage-container">
    <div class="docpage-navigation" id="toc-menu" >
        <div class="affix" data-offset-top="0">
            <% if (locals.toc) { %>
            <nav class="nav-sidemenu hidden-print hidden-sm hidden-xs" id="doc-global-toc">
                <% let headersList = headersNavigation; let toc = locals.toc; %>
                <% include partial/doc-header-main-toc.ejs %>
            </nav>
            <% } %>
            <nav class="nav-sidemenu hidden-print hidden-sm hidden-xs" data-spy="affix" id="doc-local-toc">
                <% let headersList = headersNavigation; let headersLevel = 1; %>
                <% include partial/doc-header-nav-list.ejs %>
            </nav>
        </div>
    </div>
    <div class="docpage-content" id="doc-content">
        <%- docPageContent %>
    </div>
</div>

<script type="text/javascript">
    $(function() {
        $(window).scroll(function(){
            if($(window).scrollTop() > 10) {
                $("a.page-top").show();
            }
            else {
                $("a.page-top").hide();
            }
        }).trigger('scroll');
        $('body').scrollspy({ target: '#toc-menu' , offset: 78});
    });
</script>