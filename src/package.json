{
  "name": "3DPTools",
  "version": "1.5.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "startmon": "nodemon ./bin/www",
    "bower": "bower",
    "cli": "node ./bin/cli",
    "test": "mocha -c --recursive",
    "snyk-test": "snyk test",
    "coverage": "nyc yarn test",
    "coverage-colorless": "nyc --no-color mocha --recursive -R spec -c",
    "snyk-protect": "snyk protect",
    "disabled-prepare": "yarn run snyk-protect"
  },
  "engines": {
    "node": ">=12"
  },
  "nyc": {
    "cache": true,
    "all": true,
    "report-dir": "./coverage",
    "reporter": [
      "text-summary",
      "lcov",
      "html"
    ],
    "exclude": [
      "coverage/**",
      "test/**",
      "**/node_modules/**",
      "public/**",
      "tmp/**"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "public/",
      "converage/",
      ".nyc_output"
    ]
  },
  "dependencies": {
    "@sentry/browser": "^6.2.0",
    "@sentry/node": "^6.2.0",
    "app-module-path": "^2.2.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "bower": "^1.8.12",
    "color": "^3.1.3",
    "colors": "^1.4.0",
    "commander": "^7.1.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-redis": "^5.1.0",
    "consign": "^0.1.6",
    "cookie-parser": "^1.4.5",
    "diskusage": "^1.1.3",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-form": "^0.12.6",
    "express-prom-bundle": "^6.3.3",
    "express-session": "^1.17.1",
    "express-winston": "^4.1.0",
    "fs-extra": "^9.1.0",
    "fs-promise": "^2.0.3",
    "gm": "^1.23.1",
    "hjs": "0.0.6",
    "hogan.js": "^3.0.2",
    "html-entities": "^2.1.0",
    "i18n": "^0.13.2",
    "js-yaml": "^4.0.0",
    "marked": "^2.0.0",
    "merge": "^2.1.0",
    "moment": "^2.29.1",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.17",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nconf": "^0.11.2",
    "node-schedule": "^2.0.0",
    "nodemailer": "^6.4.18",
    "parse-duration": "^0.4.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "pkginfo": "^0.4.1",
    "prom-client": "^13.1.0",
    "redis": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "snyk": "^1.458.0",
    "when": "^3.7.8",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "debug": "^4.3.1",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "should": "^13.2.3"
  },
  "snyk": true
}
