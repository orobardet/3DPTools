{
  "name": "3DPTools",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "startmon": "nodemon ./bin/www",
    "bower": "bower",
    "cli": "node ./bin/cli",
    "test": "mocha -c --recursive",
    "coverage": "nyc npm test",
    "coverage-colorless": "nyc --no-color mocha --recursive -R spec -c"
  },
  "nyc": {
    "cache": true,
    "all": true,
    "report-dir": "./coverage",
    "reporter": [
      "text-summary",
      "lcov",
      "html"
    ],
    "exclude": [
      "coverage/**",
      "test/**",
      "**/node_modules/**",
      "public/**",
      "tmp/**"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "public/",
      "converage/",
      ".nyc_output"
    ]
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "bower": "^1.8.2",
    "color": "^2.0.0",
    "colors": "^1.1.2",
    "commander": "^2.11.0",
    "connect-flash": "^0.1.1",
    "connect-redis": "^3.3.2",
    "consign": "^0.1.6",
    "cookie-parser": "^1.4.3",
    "diskusage": "^0.2.4",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-ejs-layouts": "^2.3.1",
    "express-form": "^0.12.6",
    "express-prom-bundle": "^3.2.1",
    "express-session": "^1.15.6",
    "fs-promise": "^2.0.3",
    "gm": "^1.23.0",
    "hjs": "0.0.6",
    "hogan.js": "^3.0.2",
    "i18n": "^0.8.3",
    "js-yaml": "^3.10.0",
    "kerberos": "0.0.23",
    "marked": "^0.3.6",
    "merge": "^1.2.0",
    "moment": "^2.19.1",
    "mongoose": "^4.12.6",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nconf": "^0.8.5",
    "node-schedule": "^1.2.5",
    "nodemailer": "^4.3.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.4.0",
    "raven": "^2.2.1",
    "serve-favicon": "^2.4.5",
    "when": "^3.7.8"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "debug": "^3.1.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "should": "^13.1.3"
  }
}
